
@{
    ViewBag.Title = "ProductList";
}

@model IEnumerable<SeleniumMvc.Models.ORM.Entity.Product>

<div class="col-md-12">
    <table class="table table-hover">
        <thead>
            <tr>
                <th>Ürün Adı</th>
                <th>Ürün Fiyatı</th>
                <th>Ürün Stok Miktarı</th>

            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Name</td>
                    <td>@item.UnitPrice</td>
                    <td>@item.UnitsInStock</td>
                </tr>
            }
        </tbody>
    </table>
    </div>
<a id="Add" href="~/Product/Add">Ekle</a>




